    <section class="page-title style2 parallax parallax1">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="page-title-heading">
              Discover the spaces
            </div>
            <div class="text-heading">
              Explore spaces
            </div>
            <div class="wrap-box-search">
              <form action="/spaces" method="get" accept-charset="utf-8">
                <span class="categories">
                  <span class="ti-angle-down"></span>
                  <select name="search[properties][]">
                    <option value="" disabled selected>What are you planning?</option>
                    <% Property.activity_types.each do |prop| %>
                      <option value="<%= prop.id %>"><%= prop.content.capitalize %></option>
                    <% end %>
                  </select>
                </span>
                <span class="location">
                  <span class="ti-location-pin"></span>
                  <input type="text" placeholder="Location" name="search[location]" id="space_address_autocomplete">
                </span>
                <button type="submit" class="search-btn">Search</button>
              </form><!-- /form -->
            </div><!-- /.wrap-box-search -->
          </div><!-- /.col-md-12 -->
        </div><!-- /.row -->
      </div><!-- /.container -->
      <div class="overlay"></div>
    </section><!-- /.page-title -->

